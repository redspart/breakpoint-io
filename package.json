{
  "name": "breakpointio",
  "displayName": "BreakpointIO",
  "description": "Allows you to export and import your breakpoints",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:extension.breakpoint_export",
    "onCommand:extension.breakpoint_load"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.breakpoint_export",
        "title": "export breakpoints"
      },
      {
        "command": "extension.breakpoint_import",
        "title": "import breakpoints"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/sinon": "^7.0.13",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "sinon": "^7.3.2"
  }
}
